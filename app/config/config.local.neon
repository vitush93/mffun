parameters:

extensions:
    console: Kdyby\Console\DI\ConsoleExtension
    events: Kdyby\Events\DI\EventsExtension
    annotations: Kdyby\Annotations\DI\AnnotationsExtension
    doctrine: Kdyby\Doctrine\DI\OrmExtension

doctrine:
    host: localhost
    user: root
    password:
    dbname: rocnikac
    metadataCache: default
    metadata:
        App: %appDir%

services:
    # repositories
    - App\Model\Repositories\QuoteRepository
    - App\Model\Repositories\UserRepository
    - App\Model\Repositories\TeacherRepository
    - App\Model\Repositories\SubjectRepository
    - App\Model\Repositories\TagRepository

    # services
    - App\Model\Services\AuthenticationService
    - App\Model\Services\QuoteManagementService

    # factories
    - App\RouterFactory
    - App\AdminModule\IGridoUsersControlFactory
    - App\FrontModule\Components\AddQuote\IAddQuoteControlFactory